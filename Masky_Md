
 

FROM quay.io/Masky01bot/Masky_Bot
RUN git clone https://github.com/Masky01bot/Masky_Bot /root/Maskymd
WORKDIR /root/Maskymd
RUN npm install
EXPOSE 8000
CMD ["npm","start" ] 


































































































































































































